#include <stdio.h>

long long toBinary(int n) {
    if (n == 0)
        return 0;
    if (n == 1)
        return 1;
    return toBinary(n / 2) * 10 + n % 2;
}

int main() {
    int n;
    printf("Unesi nenegativan broj: ");
    scanf("%d", &n);
    
    if (n < 0) {
        printf("Broj treba da bude nenegativan!\n");
    } else {
        printf("%lld\n", toBinary(n));
    }
    
    return 0;
}